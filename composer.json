{
  "name": "aymanalhattami/laravel-approval",
  "description": "Attach approvals to any model to prevent unauthorised updates.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Stephen Lake",
      "email": "stephen@closurecode.com"
    },
    {
      "name": "Ayman Alhattami",
      "email": "ayman.m.alhattami@gmail.com"
    }
  ],
  "require-dev": {
    "php": "^7.0|^7.1|^7.2|^7.3|^7.4|^8.0|^8.1|^8.2",
    "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|^6.0|^7.0|^8.0|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Approval\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Approval\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Approval\\ApprovalServiceProvider"
      ]
    }
  }
}
